var Vec=(x,y)->{
  this.x=x;
  this.y=y;
  this
};
Vec.len=(self)->{
  sqrt(self.x*self.x+self.y*self.y)
};
var a=Vec(2,3);
print(a);
var b=Vec(4,3);
print(a,b,a.len(a));
