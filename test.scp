var sqrt=(x)->{
  n=1;
  for(var i=0;i<64;i=i+1){
    n=(n+x/n)/2;
  }
  n
};
var sqrt3=sqrt(3);
var rows=40;
for(var i=0;i<rows;i=i+1){
  var s="";
  for(var j=0;j<rows;j=j+1/2){
    var x=(j)/rows*2-1;
    var y=(i)/rows*2-1;
    var d=sqrt(x*x+y*y);
    if(d*rows%10<2){
      s=s+"O";
    }else{
      s=s+" ";
    }
  }
  s=s+"";
  print(s);
}
