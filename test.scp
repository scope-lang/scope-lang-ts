var mandelbrot=(cx,cy)->{
  var X=cx+0;
  var Y=cy+0;
  var breok=false;
  for(var i=0;i<20;i=i+1){
    if(X*X+Y*Y>4){
      breok=true;
    }

    var zx=X*X-Y*Y;
    var zy=2*Y*X;
    Y=zy+cy;

    X=zx+cx;
  }
  breok
}


var rows=40;
for(var i=0;i<rows;i=i+1){
  var s="";
  for(var j=0;j<rows;j=j+1/2){
    var x=(j)/rows*2-1;
    var y=(i)/rows*2-1;
    var d=mandelbrot(x*2,y*2);
    if(d){
      s=s+"O";
    }else{
      s=s+" ";
    }
  }
  s=s+"";
  print(s);
}
